{
  "name": "revamp",
  "version": "1.0.0",
  "description": "Legacy browser compatibility proxy - Transform modern web for iOS 9+ (iPad 2) and older devices",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "build": "rm -rf .revamp-cache && tsc",
    "clean": "rm -rf dist .revamp-cache .revamp-certs",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:unit": "vitest",
    "test:unit:run": "vitest run"
  },
  "keywords": [
    "proxy",
    "socks5",
    "babel",
    "postcss",
    "legacy",
    "ios",
    "compatibility"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/babel__core": "^7.20.5",
    "@types/node": "^24.10.1",
    "@types/node-forge": "^1.3.14",
    "@types/sharp": "^0.32.0",
    "playwright": "^1.57.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "cheerio": "^1.1.2",
    "node-forge": "^1.3.2",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.4.0",
    "sharp": "^0.34.5"
  }
}
